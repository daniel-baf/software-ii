{
    "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
    "_comment": "This config was generated using a preset. Please see the handbook for more information: https://stryker-mutator.io/docs/stryker-js/guides/",
    "mutate": [
        "src/**/*.ts",
        "!src/types.ts",
        "!src/**/*.test.ts"
    ],
    "ignorePatterns": [
        "import",
        "export"
    ],
    "testRunner": "jest",
    "jest": {
        "projectType": "custom",
        "configFile": "jest.config.js",
        "enableFindRelatedTests": true
    },
    "reporters": [
        "progress",
        "clear-text",
        "html"
    ],
    "coverageAnalysis": "perTest",
    "checkers": [
        "typescript"
    ]
}